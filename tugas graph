class Peta: 
    def __init__(self):
        self.cityList = {}

    def printPeta(self):
        for kota in self.cityList:
            print(kota, ":", self.cityList[kota])

    def tambahkanKota(self, kota):
        if kota not in self.cityList:
            self.cityList[kota] = []
            return True
        return False
    
    def hapusKota(self, kotaDihapus):
        if kotaDihapus in self.cityList:
            # Hapus kotaDihapus dari setiap kota lain
            for kotalain in self.cityList:
                # Cek apakah kota yang ingin dihapus ada jalannya ke kota lain
                if kotaDihapus in self.cityList[kotalain]:
                    self.cityList[kotalain].remove(kotaDihapus)
            # Hapus kotaDihapus dari daftar kota
            del self.cityList[kotaDihapus]
            return True
        return False
    
    def tambahkanJalan(self, kota1, kota2):
        if kota1 in self.cityList and kota2 in self.cityList:
            # Masukkan kota1 di list kota2
            self.cityList[kota2].append(kota1)
            # Masukkan kota2 di list kota1
            self.cityList[kota1].append(kota2)
return True
        return False
    
    def hapusJalan(self, kota1, kota2):
        if kota1 in self.cityList and kota2 in self.cityList:
            # Hapus kota1 di list kota2
            self.cityList[kota2].remove(kota1)
            # Hapus kota2 di list kota1
            self.cityList[kota1].remove(kota2)
            return True
        return False

petaIndonesia = Peta()
petaIndonesia.tambahkanKota("Semarang")
petaIndonesia.tambahkanKota("Salatiga")
petaIndonesia.tambahkanKota("Klaten")
petaIndonesia.tambahkanKota("Purworejo")
petaIndonesia.tambahkanKota("Kebumen")
petaIndonesia.tambahkanKota("Purwokerto")
petaIndonesia.tambahkanKota("Tegal")
petaIndonesia.tambahkanKota("Pemalang")
petaIndonesia.tambahkanKota("Pekalongan")
petaIndonesia.tambahkanKota("Wonosobo")
petaIndonesia.tambahkanKota("Sukoharjo")
petaIndonesia.tambahkanKota("Purwodadi")
petaIndonesia.tambahkanKota("Kudus")
petaIndonesia.tambahkanKota("Jepara")
petaIndonesia.tambahkanJalan("Semarang", "Salatiga")
petaIndonesia.tambahkanJalan("Salatiga", "Klaten")
petaIndonesia.tambahkanJalan("Klaten", "Purworejo")
petaIndonesia.tambahkanJalan("Purworejo", "Kebumen")
petaIndonesia.tambahkanJalan("Kebumen", "Purwokerto")
petaIndonesia.tambahkanJalan("Purwokerto", "Tegal")
petaIndonesia.tambahkanJalan("Tegal", "Pemalang")
petaIndonesia.tambahkanJalan("Pemalang", "Pekalongan")
petaIndonesia.tambahkanJalan("Pekalongan", "Wonosobo")
petaIndonesia.tambahkanJalan("Wonosobo", "Sukoharjo")
petaIndonesia.tambahkanJalan("Sukoharjo", "Purwodadi")
petaIndonesia.tambahkanJalan("Purwodadi", "Kudus")
petaIndonesia.tambahkanJalan("Kudus", "Jepara")
petaIndonesia.tambahkanJalan("Jepara", "Klaten")
petaIndonesia.tambahkanJalan("Purwokerto", "Pemalang")
petaIndonesia.tambahkanJalan("Kebumen", "Pekalongan")
petaIndonesia.tambahkanJalan("Wonosobo", "Kebumen")
petaIndonesia.tambahkanJalan("Salatiga", "Wonosobo")
petaIndonesia.tambahkanJalan("Salatiga", "Sukoharjo")
petaIndonesia.tambahkanJalan("Purwodadi", "Salatiga")
petaIndonesia.printPeta()
print('------------------------------')
petaIndonesia.hapusKota("Semarang")
petaIndonesia.printPeta()
